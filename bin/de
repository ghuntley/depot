#!/usr/bin/env bash
# Copyright (c) 2020 Geoffrey Huntley <ghuntley@ghuntley.com>. All rights reserved.
# SPDX-License-Identifier: Proprietary

set -Eeuo pipefail

if [ $# -lt 1 ]; then
   "${REPO_ROOT}/bin/de-help"
else
  cmd="$1"
  exe="${REPO_ROOT}/bin/de-${cmd}"
  if [ -x "$exe" ]; then
      shift
      exec "$exe" "$@"
  else
      echo "Unrecognized command ${cmd}, executable ${exe}."
      exit 1
  fi
fi
